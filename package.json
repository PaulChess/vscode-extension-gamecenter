{
	"name": "gamecenter",
	"displayName": "gamecenter",
	"description": "vscode game",
  "publisher": "shenjiaqi",
	"version": "0.0.1",
  "repository":"https://github.com/paulChess/vscode-extension-gamecenter",
	"engines": {
		"vscode": "^1.47.0"
	},
  "icon": "logo.png",
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:gamecenter.helloWorld"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "gamecenter.helloWorld",
				"title": "Hello World"
			}
		],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "gamecenter",
          "title": "小霸王",
          "icon": "resources/gamecenter.svg"
        }
      ]
    },
    "views": {
      "gamecenter": [
        {
          "id": "romLocal",
          "name": "localGame"
        },
        {
          "id": "romRemote",
          "name": "remoteGame"
        }
      ]
    }
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.47.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	}
}
